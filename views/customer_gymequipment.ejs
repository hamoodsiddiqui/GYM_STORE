      <div class="uren-cart-area">
        <div class="container-fluid">
          <div class="row">
            <div class="col-12">
              <div class="table-content table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                      <th class="uren-product-remove">S.No</th>
                      <th class="uren-product-thumbnail">images</th>
                      <th class="cart-product-name">Product</th>
                      <th class="uren-product-price">Unit Price</th>
                      <th class="uren-product-quantity">Quantity</th>
                      <th class="uren-product-subtotal">Total</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% if (data.length === 0) { %>
                    <p>Your cart is empty.</p>
                    <% } else { %> <% data.forEach((item, index) => { %>
                    <tr>
                      <td class="uren-product-remove">
                        <p><%= index + 1 %></p>
                      </td>
                      <td class="uren-product-thumbnail">
                        <a href="/gymequipment/details/<%= item.product_id %>"
                          ><img
                            src="http://localhost:3000/<%= item.imageurl %>"
                            alt="<%= item.product_name %> Image"
                        /></a>
                      </td>
                      <td class="uren-product-name">
                        <a href="javascript:void(0)"
                          ><%= item.product_name %></a
                        >
                      </td>
                      <td class="uren-product-price">
                        <span class="amount"><%= item.price %></span>
                      </td>
                      <td class="quantity">
                        <p><%= item.quantity %></p>
                      </td>
                      <td class="product-subtotal">
                        <span class="amount"
                          ><%= item.price * item.quantity %></span
                        >
                      </td>
                    </tr>
                    <% }); %> <% } %>
                  </tbody>
                </table>
              </div>
              <div class="row">
                <div class="col-md-5 ml-auto">
                  <div class="cart-page-total">
                    <h2>Cart totals</h2>

