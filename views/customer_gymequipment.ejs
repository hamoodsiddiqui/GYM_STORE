                      <!-- <a href="/views/checkout.ejs"</a> -->
                      <button class="btn btn-primary" type="submit">
                        Proceed to Checkout
                      </button>
                    </form>
                    <form action="/cart/delete" method="post">
                      <button class="btn btn-danger mt-3" type="submit">
                        Delete
                      </button>
                    </form>
                  </div>
                </div>
              </div>
            </div>
